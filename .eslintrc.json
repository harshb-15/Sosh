{
    // "env": {
    //     "node": true,
    //     "commonjs": true,
    //     "es2021": true
    // },
    "extends": ["airbnb", "prettier","plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-vars":"warn",
        "no-console":"off",
        "quotes":"warn",
        "object-shorthand": "off",
        "func-names":"off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "dependencies": true}]
    }
}